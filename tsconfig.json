{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "assets/*": [
        "src/assets/*"
      ],
      "assets/icons/*": [
        "src/assets/icons/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "helpers/*": [
        "src/helpers/*"
      ]
    },
    "allowImportingTsExtensions": true,
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    },
    "include": ["./src/**/*", "./assets/**/*", "./components/**/*",
      "./gatsby-node.ts", "./gatsby-config.ts", "./gatsby-browser.tsx", "./gatsby-ssr.tsx",
      "./gatsby-types.d.ts"]
    }
